<!-- Pricing Toggle Script -->
<script>
  document.addEventListener('DOMContentLoaded', function() {
    const toggle = document.getElementById('pricing-toggle');
    const circle = document.getElementById('toggle-circle');
    const monthlyLabel = document.getElementById('monthly-label');
    const annualLabel = document.getElementById('annual-label');

    const starterPrice = document.getElementById('starter-price');
    const professionalPrice = document.getElementById('professional-price');
    const enterprisePrice = document.getElementById('enterprise-price');

    const starterNote = document.getElementById('starter-annual-note');
    const professionalNote = document.getElementById('professional-annual-note');
    const enterpriseNote = document.getElementById('enterprise-annual-note');

    let isAnnual = false;

    toggle.addEventListener('click', function() {
      isAnnual = !isAnnual;

      if (isAnnual) {
        toggle.classList.add('bg-primary-600');
        toggle.classList.remove('bg-gray-300');
        circle.classList.add('translate-x-7');
        monthlyLabel.classList.add('text-gray-500');
        monthlyLabel.classList.remove('text-gray-900');
        annualLabel.classList.add('text-gray-900');
        annualLabel.classList.remove('text-gray-500');

        starterPrice.textContent = '159';
        professionalPrice.textContent = '399';
        enterprisePrice.textContent = '799';

        starterNote.classList.remove('hidden');
        professionalNote.classList.remove('hidden');
        enterpriseNote.classList.remove('hidden');
      } else {
        toggle.classList.remove('bg-primary-600');
        toggle.classList.add('bg-gray-300');
        circle.classList.remove('translate-x-7');
        monthlyLabel.classList.remove('text-gray-500');
        monthlyLabel.classList.add('text-gray-900');
        annualLabel.classList.remove('text-gray-900');
        annualLabel.classList.add('text-gray-500');

        starterPrice.textContent = '199';
        professionalPrice.textContent = '499';
        enterprisePrice.textContent = '999';

        starterNote.classList.add('hidden');
        professionalNote.classList.add('hidden');
        enterpriseNote.classList.add('hidden');
      }
    });
  });
</script>
