              <option value="">Select a service...</option>
              <option value="form-990">Form 990 Filing</option>
              <option value="bookkeeping">Bookkeeping</option>
              <option value="grant-accounting">Grant Accounting</option>
              <option value="payroll">Payroll Services</option>
              <option value="compliance">Compliance & Audit Support</option>
              <option value="tax-prep">Tax Preparation</option>
              <option value="consulting">Financial Consulting</option>
              <option value="501c3-setup">501(c)(3) Setup</option>
