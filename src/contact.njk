              calInline.src = 'https://app.cal.com/embed/embed.js';
              var s = document.getElementsByTagName('script')[0];
              s.parentNode.insertBefore(calInline, s);

              calInline.onload = function() {
                const embedPlaceholder = document.getElementById('cal-embed-placeholder');
                if (embedPlaceholder) {
                  embedPlaceholder.innerHTML = '<div data-cal-link="YOUR_CAL_USERNAME/30min" data-cal-config=\'{"theme":"light"}\' style="width:100%;height:100%;overflow:scroll"></div>';
