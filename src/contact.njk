<script>
  // Capture plan parameter from URL
  document.addEventListener('DOMContentLoaded', function() {
    const urlParams = new URLSearchParams(window.location.search);
    const plan = urlParams.get('plan');
    if (plan) {
      document.getElementById('plan-input').value = plan;
      // Pre-select service if plan is specified
      const serviceMap = {
        'starter': 'bookkeeping',
        'professional': 'form-990',
        'enterprise': 'consulting',
        'custom': 'other'
      };
      if (serviceMap[plan]) {
        document.getElementById('service').value = serviceMap[plan];
      }
    }
  });
</script>
